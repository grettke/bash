function nthlineof {
  if (( "$#" < 2 )); then
      echo 'Print the NTH line of FILE'
      echo "Usage: ${FUNCNAME[0]} NTH FILE"
      return 1
  fi
  tail -n+"$1" "$2" | head -n1
}
