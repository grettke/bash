function xwnhom {
  local gravity=0;
  local x=100;
  local y=100;
  local w=200;
  local h=200;
  if [[ $# -eq 0 || -z "$1" ]] ; then
      printf "Usage: ${FUNCNAME[0]} <WindowTitle>\n"
      printf "Moves window to $x,$y and resizes it to $w,$h.\n"
      return 1
  fi
  local window_name=$1;
  wmctrl -r "$window_name" -e "$gravity","$x","$y","$w","$h"
}
