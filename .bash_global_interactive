# [[file:~/git/github/bash/Provision.org::org_gcr_2017-05-12_mara_347A5539-7A7D-4802-8147-B3AE55F068F5][org_gcr_2017-05-12_mara_347A5539-7A7D-4802-8147-B3AE55F068F5]]
emaj=4
emin=3
amaj=${BASH_VERSINFO[0]}
amin=${BASH_VERSINFO[1]}
if [[ $amaj -lt $emaj || $amin -lt $emin ]]; then
    printf "\e[31mFYI: Using Bash v$amaj.$amin.x but expecting Bash >= v$emaj.$emin.x\e[0m\n" 1>&2
fi
unset emaj
unset emin
unset amaj
unset amin
